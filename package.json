{
    "name": "youtube-mp3-uploader-gui",
    "version": "0.6.0",
    "main": "main.js",
    "devDependencies": {
        "electron": "^1.7.10",
        "electron-builder": "^19.49.2"
    },
    "dependencies": {
        "ffbinaries": "^1.0.2",
        "fluent-ffmpeg": "^2.1.2",
        "fs": "0.0.1-security",
        "googleapis": "^39.1.0",
        "lien": "^3.1.0",
